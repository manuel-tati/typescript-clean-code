{
  "name": "typescript-clean-code",
  "version": "1.0.0",
  "main": "main.ts",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/supertest": "^2.0.12",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.5",
    "@babel/preset-typescript": "^7.18.6",
    "eslint": "^8.19.0",
    "husky": "8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "jest": "^28.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "test": "jest --watchAll --collectCoverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  }
}
